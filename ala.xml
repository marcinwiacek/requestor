<project>
  <step>
    <type>template</type>
    <name>google</name>
    <value>https://www.google.com/search?q=</value>
  </step>
  <step>
    <type>template</type>
    <name>duck</name>
    <value>https://duckduckgo.com/?q=</value>
  </step>
  <step>
    <type>forEach</type>
    <value>engine,word\n${google},test\n${duck},test</value>
    <step>
       <type>http1get</type>
       <url>${engine}${word}</url>
       <headers>Content-Type:charset=UTF-8</headers>
    </step>
  </step>
</project>
